:custome
 cls
 ECHO Add the file type. (EX: .bin)
 ECHO Don't add ":" after your usb letter.
 ping 1.1.1.1 -n 1 -w 1250> nul
 ECHO .
 ECHO .
 ECHO .
 SET /P uletter= What is your usb letter?
 ECHO Looking for USB
 ping 1.1.1.1 -n 1 -w 1500> nul
 ECHO found
 ping 1.1.1.1 -n 1 -w 300> nul
 SET /P fname= What is the file you want to delete?
 ECHO Looking for file
 %uletter%:
 ping 1.1.1.1 -n 1 -w 1500> nul
 IF EXIST %fname% GOTO :fexist
 IF NOT EXIST %fname% GOTO :fnotexist
 
:fnotexist
ping 1.1.1.1 -n 1 -w 1200> nul
ECHO File not found.
ECHO Either your file "%fname%" was not found or your usb drive "%letter%" was not found.
ECHO Redirecting...
ping 1.1.1.1 -n 1 -w 1350> nul
GOTO :exit

 
:fexist
ECHO file found.
ping 1.1.1.1 -n 1 -w 1100> nul
ECHO Deleting File
ping 1.1.1.1 -n 1 -w 1300> nul 
%uletter%:
del /f %fname%
ping 1.1.1.1 -n 1 -w 350> nul
ECHO Done deleting files.
ECHO Redirecting..
ping 1.1.1.1 -n 1 -w 700> nul
GOTO :exit
 


:rubberDEL
set /P drive= What is the letter of your drive?
ECHO Looking for %drive%: drive
ping 1.1.1.1 -n 1 -w 2000> nul
ECHO Looking "inject.bin"
ping 1.1.1.1 -n 1 -w 900> nul
%drive%:
IF EXIST inject.bin GOTO :inexist
IF NOT EXIST inject.bin GOTO :noexist

:noexist
ping 1.1.1.1 -n 1 -w 1000> nul
ECHO Couldn't find "inject.bin" or "%drive%:"
ECHO Need Help?
set /P yas= To trouble shoot type Y. To exit type N.(Y/N)
IF %yas%==Y GOTO :help
IF %yas%==N GOTO :exit

:help
cls
ECHO (1) Make sure of what your drive letter is. Not your name.
ECHO ...........................................................
ECHO (2) Don't put ":" after your letter.
ECHO ...........................................................
ECHO (3) Are you sure there is a "inject.bin" file in your usb?
ECHO ...........................................................
ECHO (4) Did you forget to put your file type at the end of your file name?
ECHO ...........................................................
ECHO Type Y to go back to menu or anything else to exit.
SET /P helpe= ---------------------------------------------------
IF %helpe%==Y GOTO :start


:inexist
ECHO found "inject.bin"
ping 1.1.1.1 -n 1 -w 1000> nul
ECHO deleting "inject.bin" from "%drive%:"
